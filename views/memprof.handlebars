<div class="flex">
    <div class="w-1/2  h-12 pics">
        <div class="max-w-sm rounded overflow-hidden shadow-lg pcard">

            <div class="px-6 py-4">
                <div class="flex">
                    <div class="w-1/2">
                        <div class="font-bold text-xl mb-2">Picture(s)</div>
                    </div>
                    <div class="w-1/2">
                        <button
                            class="submitbutton hover:bg-blue-400 text-white font-bold py-2 px-4 border-b-4  hover:border-blue-500 rounded">
                            Upload
                        </button>
                    </div>
                </div>
            </div>
            <div class="px-6 py-4">
                <input placeholder="Upload your files here" class="input" id="picupload"></input>
            </div>
            <div class="px-6 py-4">

            </div>
        </div>
    </div>
    <div class="w-1/2  h-12 un">
        <div class="max-w-sm rounded overflow-hidden shadow-lg namecard">
            <div class="px-6 py-4">
                <div class="flex">
                    <div class="w-1/2">
                        <div class="font-bold text-xl mb-2">Name</div>
                    </div>
                    <div class="w-1/2">
                        <button
                            class="submitbutton hover:bg-blue-400 text-white font-bold py-2 px-4 border-b-4  hover:border-blue-500 rounded"
                            id="savename">
                            Save Name
                        </button>
                    </div>
                </div>
            </div>
            <div class="px-6 py-4">
                <input placeholder="First Name" class="input" id="fninput">{{member.first_name}}</input>
            </div>
            <div class="px-6 py-4">
                <input placeholder="Last Name" class="input" id="lninput">{{member.last_name}}</input>
            </div>

        </div>
    </div>

</div>
<div class="flex row2">
    <div class="w-1/3 h-12 pinfo">
        <div class="rounded overflow-hidden shadow-lg row2card">
            <div class="px-6 py-4">
                <div class="flex">
                    <div class="w-1/2">
                        <div class="font-bold text-xl mb-2">Edit your personal info:</div>
                    </div>
                    <div class="w-1/2">
                        <button
                            class="submitbutton hover:bg-blue-400 text-white font-bold py-2 px-4 border-b-4  hover:border-blue-500 rounded"
                            id="aboutsubmit">
                            Commit
                        </button>
                    </div>
                </div>
            </div>
            <div class="px-6 py-4">
                <input placeholder="" class="row2input" id="aboutinput">{{member.about_me}}</input>
            </div>
        </div>
    </div>
    <div class="w-1/3 h-12">
        <div class="rounded overflow-hidden shadow-lg row2card">
            <div class="rounded overflow-hidden shadow-lg row2card">
